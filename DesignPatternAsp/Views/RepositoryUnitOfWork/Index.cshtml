@model IEnumerable<DesignPatternAsp.Models.ViewModels.TurnosViewModel>

<div class="text-center">
    <div>
        <a class="btn btn-primary" href="~/Medico/Medico" >Agregar Turno</a>
        @foreach (var turno in Model)
        {
            <p>@turno.IdMedico</p>
            <p>@turno.Duracion</p>
            <p>@turno.Fecha</p>
            <p>@turno.IdPaciente</p>
        }
    </div>
</div>
